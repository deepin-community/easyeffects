                                                            <property name="row">0</property>
                                                        </layout>
                                                    </object>
                                                </child>
                                                <child>
                                                    <object class="GtkSpinButton" id="dry">
                                                        <property name="valign">center</property>
                                                        <property name="width-chars">10</property>

                                                                        <property name="valign">center</property>
                                                                        <accessibility>
                                                                            <relation name="labelled-by">enable_band1_label</relation>
                                                                        </accessibility>
                                                                    </object>
                                                                </child>
                                                            </object>
                                                        </child>
                                                    </object>
                                                </child>

                                                <child>
                                                    <object class="GtkListBoxRow">

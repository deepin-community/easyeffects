                                                            <property name="row">1</property>
                                                        </layout>
                                                        <accessibility>
                                                            <relation name="labelled-by">gate_mode_label</relation>
                                                        </accessibility>
                                                    </object>
                                                </child>

                                                <child>
                                                    <object class="GtkLabel" id="dry_label">
                                                        <property name="label" translatable="yes">Dry Level</property>
